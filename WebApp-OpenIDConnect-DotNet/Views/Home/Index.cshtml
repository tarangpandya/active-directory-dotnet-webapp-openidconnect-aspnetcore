@model IEnumerable<System.Security.Claims.Claim>
    @{
    ViewData["Title"] = "Home Page";
    }

    <div class="row">
        <div class="col-md-12">
            <table>
                <thead>
                    <tr>
                        <th>Claim</th>
                        <th>Value</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach(var claim in Model)
                    {
                    <tr>
                        <td>@claim.Type</td>
                        <td>@claim.Value</td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

